programa
{
	
inclua biblioteca Util --> u
	funcao inicio()
	{
		inteiro n1, n2

		escreva("Informe um número inteiro: ")
		leia (n1)
		escreva("Informe outro número inteiro: ")
		leia (n2)

		se(n1 < n2){
			escreva(n1, " ")
			enquanto(n1<n2){
				n1 = n1 + 1
				escreva(n1, " ")
				Util.aguarde(500)
			}
		}
		     senao{
			escreva(n1, " ")
			enquanto(n1>n2){
			n1 = n1 - 1
			escreva(n1, " ")
			Util.aguarde(500)
		  }	
		}
	}
}
